{
  "version": 2,
  "builds": [
    {
      "src": "backend.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["backend.js", "run-command.mjs", "views/**", "src/**", "lib/rammerhead/**", "proxyServiceValidator.js"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/backend.js"
    }
  ],
  "build": {
    "env": {
      "NPM_CONFIG_PRODUCTION": "false"
    },
    "scripts": {
      "build": "node run-command.mjs build && cd lib/rammerhead && npm install && npm run build"
    }
  }
}